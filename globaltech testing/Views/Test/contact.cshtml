@model globaltech_testing.ViewModel.User
@{
    ViewData["Title"] = "contact";
}
<div class="container">
    <form asp-action="contact" asp-controller="Test">
        @Html.AntiForgeryToken()
        <div class="mb-3">
            <label for="exampleInputEmail1" class="form-label">Username</label>
            <input type="text" asp-for="Name" class="form-control" id="exampleInputEmail1">
        <div class="mb-3">
            <label for="exampleInputEmail1" class="form-label">Email address</label>
            <input type="email" asp-for="Email" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp">
            <div id="emailHelp" class="form-text">We'll never share your email with anyone else.</div>
        </div>
        <div class="mb-3">
            <label for="exampleInputPassword1" class="form-label">Password</label>
            <input type="password" asp-for="Password" class="form-control" id="exampleInputPassword1">
        </div>
        <button type="submit" class="btn btn-primary">Submit</button>
    </form>
    
</div>

